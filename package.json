{
  "name": "inbox-hero",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "export AWS_PROFILE=inbox-hero && sst bind next dev",
    "sst:dev": "export AWS_PROFILE=inbox-hero && sst dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix && prettier --write \"**/*.{js,cjs,mjs,ts,tsx,md,json}\" --ignore-path .gitignore",
    "watch": "tsc --watch",
    "db:mig:gen": "drizzle-kit generate:mysql",
    "db:push": "drizzle-kit push:mysql",
    "db:mig:push": "node -r esbuild-register src/server/database/migrate.ts",
    "db:mig:drop": "drizzle-kit drop --config=drizzle.config.ts",
    "db:studio": "drizzle-kit studio",
    "email:dev": "email dev --dir src/lib/email/templates --port 8000",
    "email:export": "email export --dir ../src/lib/email/templates --out ../src/lib/email/out",
    "preinstall": "npx only-allow pnpm"
  },
